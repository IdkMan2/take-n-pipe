{
  "name": "take-n-pipe",
  "description": "Simple tool to seamlessly chain code execution.",
  "version": "1.0.2",
  "license": "MIT",
  "homepage": "https://github.com/IdkMan2/take-n-pipe#pipe-n-take",
  "repository": "https://github.com/IdkMan2/take-n-pipe",
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup/rollup.config.js",
    "ts-check": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --loglevel warn --write \"src/**/*.ts\"",
    "clear-leavings": "rimraf *.tsbuildinfo *.eslintcache yarn-debug.log* yarn-error.log*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "types": "./dist/cjs/src/index.d.ts",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/cjs/src/index.d.ts",
        "import": "./dist/esm/src/index.d.ts"
      },
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/esm/src/index.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.3",
    "rollup": "^3.12.1",
    "rollup-plugin-delete": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  }
}
